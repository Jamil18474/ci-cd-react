{
  "name": "ci-cd-react",
  "version": "0.1.12",
  "private": false,
  "homepage": "https://jamil18474.github.io/ci-cd-react/",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@testing-library/react": "^16.0.1",
    "axios": "^1.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-npm": "cross-env NODE_ENV=production && rimraf dist && mkdir dist && npx babel src --out-dir dist --copy-files",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "test:integration": "react-scripts test --testPathPattern=integration --coverage --watchAll=false",
    "test:unit": "react-scripts test --testPathPattern=unit --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "jsdoc": "jsdoc -R ../README.md -c ./jsdoc.config.json -r -d frontend/public/docs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@babel/preset-env": "^7.25.8",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.2.0",
    "jest": "^27.5.1",
    "jsdoc": "^4.0.3",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^6.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*js",
      "!src/reportWebVitals.js",
      "!src/index.js",
      "!src/babel.config.js"
    ]
  }
}